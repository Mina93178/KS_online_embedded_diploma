#include <stdio.h>
#include <stdlib.h>
void flush(void){
	fflush(stdin);
	fflush(stdout);
}
int main(void) {
	float User_Input_1=0.0,User_Input_2=0.0;
	printf("Enter Two Numbers:");
	flush();
	scanf("%f %f",&User_Input_1,&User_Input_2);
	printf("Sum:%d",User_Input_1+User_Input_2);

	return 0;
}
